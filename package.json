{
  "name": "proshop",
  "version": "1.0.0",
  "description": "FULL STACK APPLICATION",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "build": "cd frontend && npm run build",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-ppostbuild":"NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "suresh ch",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "config": "^3.3.6"
  }
}
